@model IEnumerable<UnamiSushi.Models.MenuCategory>
@using UnamiSushi.Models


@if (Model != null)
{
    <div class="grid">
        <table cellspacing="0" width="80%">
            <thead>
                <tr>
                    <th>
                        CategoryID
                    </th>
                    <th>
                        CategoryName
                    </th>
                    <th>
                        CategoryDescription
                    </th>

                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td align="left">
                            @item.CategoryID
                        </td>
                        <td align="left">
                            @item.CategoryName
                        </td>
                        <td align="left">
                            @item.CategoryDescription
                        </td>

                    </tr>
                }
            </tbody>
        </table>

    </div>

}